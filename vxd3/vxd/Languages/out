echo VXD ICON LIST VISUAL LANGUAGE XML AND DTD GENERATOR echo =================================================== export lang="VisualAltaBPM" export imgdir="Languages/${lang}/" echo "(" >langlist.txt for i in `ls ../${imgdir}Images/*.[gGijb][pciIm][pjofF] |sed -e 's|^.*/||' | sort |uniq `; do echo $i|sed -e 's|\([^\.]*\..*\)|\1|g' |sed -e 's/\([^\.]*\)\.[^\.]*$/\1|/g';done >uniq.txt;cat uniq.txt|sort|uniq > uniqsorted.txt ; cat uniqsorted.txt |tail -1 > tail.txt; cat tail.txt >> uniqsorted.txt; cat tail.txt |tr '|' ' ' > tailtr.txt; cat uniqsorted.txt |sort |uniq -u >> langlist.txt; cat tailtr.txt >>langlist.txt; echo ")" >>langlist.txt echo echo ------------- echo LANGUAGE LIST echo langlist.txt ____ _ __ ___ _ / ___| ___ _ __ ___ _ __ __ _| |_ ___ \ \ / (_)___ _ _ __ _| | | | _ / _ \ '_ \ / _ \ '__/ _` | __/ _ \ \ \ / /| / __| | | |/ _` | | | |_| | __/ | | | __/ | | (_| | || __/ \ V / | \__ \ |_| | (_| | | \____|\___|_| |_|\___|_| \__,_|\__\___| \_/ |_|___/\__,_|\__,_|_| _ __ | | __ _ _ __ __ _ _ _ __ _ __ _ ___ ___ / _|_ __ ___ _ __ ___ | | / _` | '_ \ / _` | | | |/ _` |/ _` |/ _ \/ __| | |_| '__/ _ \| '_ ` _ \ | |__| (_| | | | | (_| | |_| | (_| | (_| | __/\__ \ | _| | | (_) | | | | | | |_____\__,_|_| |_|\__, |\__,_|\__,_|\__, |\___||___/ |_| |_| \___/|_| |_| |_| |___/ |___/ ____ _ _ __ __ _ | _ \(_)_ __ ___ ___| |_ ___ _ __ _ _ ___ / _| / _` | | | | | | '__/ _ \/ __| __/ _ \| '__| | | | / _ \| |_ | (_| | | |_| | | | | __/ (__| || (_) | | | |_| | | (_) | _| \__,_| |____/|_|_| \___|\___|\__\___/|_| \__, | \___/|_| |___/ ___ |_ _|_ __ ___ __ _ __ _ ___ ___ | || '_ ` _ \ / _` |/ _` |/ _ \/ __| | || | | | | | (_| | (_| | __/\__ \ |___|_| |_| |_|\__,_|\__, |\___||___/ |___/ echo echo =============== echo ${lang}_NEW.dtd echo =============== echo cat languagedtdtemplate.txt > ${lang}_NEW.dtd for i in `cat langlist.txt iconsLangXML.txt |sort |uniq -u |grep -v '(' |grep -v ')'|sed -e 's/|//g'`; do echo $i `grep "^$i$" dtdelements.txt |wc -l` |grep -v 1;cat elattr.txt |sed -e 's/Panel\([\" ]\)/'$i'\1/g';done >> ${lang}_NEW.dtd
