			<xsd:attribute name="onbeforeprint" vs:omtype="event" />
			<xsd:attribute name="onfilterchange" vs:omtype="event" />
			<xsd:attribute name="onscroll" vs:omtype="event" />
			<xsd:attribute name="ontimeerror" vs:omtype="event" />			
